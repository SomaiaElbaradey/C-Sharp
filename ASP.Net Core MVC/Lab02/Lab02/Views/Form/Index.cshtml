
@{
    ViewBag.Title = "Index";
}


<div class="wrapper row2">
    <section class="hoc container">
        <div class="d-flex justify-content-center">
            <form method="post" action="/Form/FormResult" enctype="multipart/form-data">
                <div class="m-2">
                    <label for="id" class="form-label">Id:</label>
                    <input type="text" name="id" class="form-control" />
                </div>
                <div class="m-2">
                    <label for="email" class="form-label">Name:</label>
                    <input type="text" id="email" name="name" class="form-control">
                </div>
                <div class="m-2">
                    <label for="age" class="form-label">Age:</label>
                    <input type="number" id="age" name="age" class="form-control">
                </div>
                <div class="m-2">
                    <label for="email" class="form-label">Email address:</label>
                    <input type="email" id="email" name="email" class="form-control">
                </div>
                <div class="m-2">
                    <label for="sport" class="form-label">Sport:</label>
                    <select name="sport[]" class="form-select" multiple="multiple">
                        <option value="football">football</option>
                        <option value="voleyball">voleyball</option>
                        <option value="tennis">tennis</option>
                    </select>
                </div>

                <div>
                    file:
                    <input type="file" name="myfile" class="form-control" />
                </div>
                <button type="submit" class="btn btn-danger m-3" >Submit</button>
            </form>

        </div>
    </section>
</div>